        "@dhis2/ui": "^10.8.2",
        "@reduxjs/toolkit": "^2.8.2",
        "deep-equal": "^2.2.3",
        "history": "^5.3.0",
        "prop-types": "^15.8.1",
        "query-string": "^9.2.1",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-redux": "^9.2.0"
    },
    "resolutions": {
